{
  "name": "rasreee.com",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "build:no-lint": "next build --no-lint",
    "check-all": "run-s type-check build:no-lint test lint:strict",
    "clean": "run-p clean:build clean:jest",
    "clean:build": "rm -rf .next tsconfig.tsbuildinfo node_modules/.cache",
    "clean:jest": "jest --clearCache",
    "dev": "next dev",
    "fix-all": "run-s format lint:fix",
    "format": "prettier '**/*.{json,md,mdx}' --write",
    "generate": "plop",
    "generate:component": "plop component",
    "generate:generator": "plop generator",
    "generate:page": "plop page",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx,.cjs,.mjs",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx,.cjs,.mjs --fix",
    "lint:strict": "pnpm run lint --quiet",
    "prepare": "husky install",
    "start": "next start",
    "test": "jest",
    "type-check": "tsc --noEmit --pretty",
    "ui:add": "source scripts/ui/add.sh"
  },
  "dependencies": {
    "@heroicons/react": "2.1.1",
    "@radix-ui/react-dropdown-menu": "2.0.6",
